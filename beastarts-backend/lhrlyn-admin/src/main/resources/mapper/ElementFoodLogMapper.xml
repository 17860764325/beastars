<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lhrlyn.cn.lhrlynadmin.user.mapper.ElementFoodLogMapper">

    <resultMap type="com.lhrlyn.cn.lhrlynadmin.user.entity.ElementFoodLog" id="elementFoodLogMap">
            <result property="id" column="id"/>
            <result property="createBy" column="create_by"/>
            <result property="createTime" column="create_time"/>
            <result property="updateBy" column="update_by"/>
            <result property="updateTime" column="update_time"/>
            <result property="currentDateToday" column="current_date_today"/>
            <result property="quantityOfHeat" column="quantity_of_heat"/>
            <result property="protein" column="protein"/>
            <result property="fat" column="fat"/>
            <result property="dietaryFiber" column="dietary_fiber"/>
            <result property="carbonWater" column="carbon_water"/>
            <result property="heavy" column="heavy"/>
            <result property="userIdOrName" column="user_id_or_name"/>
    </resultMap>

    <select id="page" resultMap="elementFoodLogMap"
            parameterType="com.lhrlyn.cn.lhrlynadmin.user.util.pageExample.PageExample">
        select
        sh.*
        from element_food_log sh
            <where>
                    <if test="id != null and id != ''">
                        and sh.id = #{id}
                    </if>
                    <if test="createBy != null and createBy != ''">
                        and sh.create_by = #{createBy}
                    </if>
                    <if test="createTime != null and createTime != ''">
                        and sh.create_time = #{createTime}
                    </if>
                    <if test="updateBy != null and updateBy != ''">
                        and sh.update_by = #{updateBy}
                    </if>
                    <if test="updateTime != null and updateTime != ''">
                        and sh.update_time = #{updateTime}
                    </if>
                    <if test="currentDateToday != null and currentDateToday != ''">
                        and sh.current_date_today = #{currentDateToday}
                    </if>
                    <if test="quantityOfHeat != null and quantityOfHeat != ''">
                        and sh.quantity_of_heat = #{quantityOfHeat}
                    </if>
                    <if test="protein != null and protein != ''">
                        and sh.protein = #{protein}
                    </if>
                    <if test="fat != null and fat != ''">
                        and sh.fat = #{fat}
                    </if>
                    <if test="dietaryFiber != null and dietaryFiber != ''">
                        and sh.dietary_fiber = #{dietaryFiber}
                    </if>
                    <if test="carbonWater != null and carbonWater != ''">
                        and sh.carbon_water = #{carbonWater}
                    </if>
                    <if test="userIdOrName != null and userIdOrName != ''">
                        and sh.user_id_or_name = #{userIdOrName}
                    </if>
            </where>
        order by sh.create_time desc
    </select>

    <delete id="deleteById">
        delete from element_food_log where id =#{id}
    </delete>

</mapper>